   "source": [
    "from sklearn.preprocessing import StandardScaler\n",
    "\n",
    "def standardize_model_features(df):\n",
    "    # Columns to exclude from standardization\n",
    "    exclude_cols = ['id', 'p_num', 'bg+1:00', 'time', 'time_of_day', 'time_of_day_encoded','time_cos','time_sin']\n",
    "    \n",
    "    # Also exclude categorical or non-numeric by prefix\n",
    "    exclude_prefixes = ['activity_past_', 'exercise_flag_']\n",
    "\n",
    "    # Get columns to exclude by prefix\n",
    "    prefix_excluded = [col for col in df.columns if any(col.startswith(prefix) for prefix in exclude_prefixes)]\n",
    "    \n",
    "    # Final exclusion list\n",
    "    all_excluded = set(exclude_cols + prefix_excluded)\n",
    "    \n",
    "    # Columns eligible for standardization\n",
    "    numeric_cols = df.select_dtypes(include=[np.number]).columns\n",
    "    feature_cols = [col for col in numeric_cols if col not in all_excluded]\n",
    "\n",
    "    # Apply StandardScaler\n",
    "    scaler = StandardScaler()\n",
    "    df_scaled = df.copy()\n",
    "    df_scaled[feature_cols] = scaler.fit_transform(df[feature_cols])\n",
    "\n",
    "    return df_scaled, scaler, feature_cols\n"
   ]
  },
