    "def evaluate_model(model, data_loader, scaler_y):\n",
    "    model.eval()\n",
    "    device = torch.device(\"cuda\" if torch.cuda.is_available() else \"cpu\")\n",
    "    model.to(device)\n",
    "\n",
    "    preds = []\n",
    "    true = []\n",
    "\n",
    "    with torch.no_grad():\n",
    "        for X_batch, y_batch in data_loader:\n",
    "            X_batch = X_batch.to(device)\n",
    "            outputs = model(X_batch).cpu().numpy()\n",
    "            preds.append(outputs)\n",
    "            true.append(y_batch.numpy())\n",
    "\n",
    "    preds = np.vstack(preds)\n",
    "    true = np.vstack(true)\n",
    "\n",
    "    # Inverse transform the predictions and true labels to original scale\n",
    "    preds_rescaled = scaler_y.inverse_transform(preds)\n",
    "    true_rescaled = scaler_y.inverse_transform(true)\n",
    "\n",
    "    mse = mean_squared_error(true_rescaled, preds_rescaled)\n",
    "    mae = mean_absolute_error(true_rescaled, preds_rescaled)\n",
    "    r2 = r2_score(true_rescaled, preds_rescaled)\n",
    "\n",
    "    print(f\"📉 MSE:  {mse:.4f}\")\n",
    "    print(f\"📏 MAE:  {mae:.4f}\")\n",
    "    print(f\"📈 R²:   {r2:.4f}\")\n",
    "    \n",
    "    return true_rescaled, preds_rescaled"
   ]
