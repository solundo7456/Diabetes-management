    "import torch\n",
    "from torch.utils.data import Dataset, DataLoader\n",
    "\n",
    "class GlucoseSequenceDataset(Dataset):\n",
    "    \"\"\"\n",
    "    PyTorch Dataset for LSTM glucose prediction.\n",
    "    Takes preprocessed X (3D: [samples, time_steps, features])\n",
    "    and y (1D or 2D target array).\n",
    "    \"\"\"\n",
    "    def __init__(self, X, y):\n",
    "        self.X = torch.tensor(X, dtype=torch.float32)\n",
    "        self.y = torch.tensor(y, dtype=torch.float32).unsqueeze(1)  # shape: [samples, 1]\n",
    "\n",
    "    def __len__(self):\n",
    "        return len(self.X)\n",
    "\n",
    "    def __getitem__(self, idx):\n",
    "        return self.X[idx], self.y[idx]\n",
    "\n"
   ]
