    "import pandas as pd\n",
    "import numpy as np\n",
    "import re\n",
    "from collections import defaultdict\n",
    "\n",
    "def preprocess_and_sort(df):\n",
    "    df['step'] = df['id'].str.extract(r'_(\\d+)$').astype(int)\n",
    "    df.sort_values(by=['p_num', 'step'], inplace=True)\n",
    "    df.reset_index(drop=True, inplace=True)\n",
    "    df['time'] = pd.to_datetime(df['time'], format='%H:%M:%S')\n",
    "    return df\n",
    "\n",
    "def aggregate_lagged_features(df, var_prefix, interval=15):\n",
    "    lag_cols = [col for col in df.columns if col.startswith(f'{var_prefix}-')]\n",
    "    col_bins = defaultdict(list)\n",
    "\n",
    "    for col in lag_cols:\n",
    "        match = re.search(r'-(\\d+):(\\d+)', col)\n",
    "        if match:\n",
    "            total_min = int(match.group(1)) * 60 + int(match.group(2))\n",
    "            bin_start = (total_min // interval) * interval\n",
    "            col_bins[bin_start].append(col)\n",
    "\n",
    "    agg_df = pd.DataFrame(index=df.index)\n",
    "    for bin_start in sorted(col_bins.keys(), reverse=True):\n",
    "        cols = col_bins[bin_start]\n",
    "        new_col = f'{var_prefix}_mean_past_{bin_start}m_to_{bin_start + interval - 5}m'\n",
    "        agg_df[new_col] = df[cols].mean(axis=1)\n",
    "\n",
    "    return agg_df\n",
    "\n",
    "def aggregate_activity_lagged(df, interval=15):\n",
    "    activity_cols = [col for col in df.columns if col.startswith(\"activity-\")]\n",
    "    col_bins = defaultdict(list)\n",
    "\n",
    "    for col in activity_cols:\n",
    "        match = re.search(r'-(\\d+):(\\d+)', col)\n",
    "        if match:\n",
    "            total_min = int(match.group(1)) * 60 + int(match.group(2))\n",
    "            bin_start = (total_min // interval) * interval\n",
    "            col_bins[bin_start].append(col)\n",
    "\n",
    "    agg_mode_df = pd.DataFrame(index=df.index)\n",
    "    agg_dur_df = pd.DataFrame(index=df.index)\n",
    "\n",
    "    def safe_mode(row):\n",
    "        values = row[row != 'no_activity']\n",
    "        if len(values) == 0:\n",
    "            return 'no_activity'\n",
    "        modes = values.mode()\n",
    "        return modes.iloc[0] if not modes.empty else 'no_activity'\n",
    "\n",
    "    for bin_start in sorted(col_bins.keys(), reverse=True):\n",
    "        cols = col_bins[bin_start]\n",
    "        mode_col = f'activity_past_{bin_start}m_to_{bin_start + interval - 5}m'\n",
    "        dur_col = f'activity_duration_min_{bin_start}m_to_{bin_start + interval - 5}m'\n",
    "\n",
    "        agg_mode_df[mode_col] = df[cols].apply(safe_mode, axis=1)\n",
    "        agg_dur_df[dur_col] = df[cols].apply(lambda row: (row != 'no_activity').sum() * 5, axis=1)\n",
    "\n",
    "    return agg_mode_df, agg_dur_df\n",
    "\n",
    "def aggregate_all_features(df, interval=15):\n",
    "    df = preprocess_and_sort(df)\n",
    "\n",
    "    modalities = ['bg', 'insulin', 'carbs', 'hr', 'steps', 'cals']\n",
    "    agg_feature_frames = []\n",
    "\n",
    "    for mod in modalities:\n",
    "        print(f\"Processing: {mod}\")\n",
    "        agg = aggregate_lagged_features(df, var_prefix=mod, interval=interval)\n",
    "        agg_feature_frames.append(agg)\n",
    "\n",
    "    print(\"Processing: activity (mode + duration)\")\n",
    "    agg_act_mode, agg_act_dur = aggregate_activity_lagged(df, interval=interval)\n",
    "    agg_feature_frames.extend([agg_act_mode, agg_act_dur])\n",
    "\n",
    "    df_agg_all = pd.concat(agg_feature_frames, axis=1)\n",
    "\n",
    "    identifier_cols = ['id', 'p_num', 'bg+1:00', 'time']\n",
    "    df_ident = df[identifier_cols]\n",
    "\n",
    "    df_final = df_ident.join(df_agg_all)\n",
    "    return df_final\n"
   ]
  },
