    "class DiabetesBiLSTM(nn.Module):\n",
    "    def __init__(self, input_size, hidden_size=128, num_layers=2, dropout=0.2):\n",
    "        super(DiabetesBiLSTM, self).__init__()\n",
    "\n",
    "        self.lstm = nn.LSTM(\n",
    "            input_size=input_size,\n",
    "            hidden_size=hidden_size,\n",
    "            num_layers=num_layers,\n",
    "            dropout=dropout,\n",
    "            batch_first=True,\n",
    "            bidirectional=True  # 🔁 Bidirectional LSTM\n",
    "        )\n",
    "\n",
    "        # Output layer input size doubles because of bidirectional\n",
    "        self.fc = nn.Sequential(\n",
    "            nn.Linear(hidden_size * 2, 64),  # ⬅️ hidden_size * 2 for bidirectional\n",
    "            nn.ReLU(),\n",
    "            nn.Dropout(dropout),\n",
    "            nn.Linear(64, 1)  # Output a single value\n",
    "        )\n",
    "\n",
    "    def forward(self, x):\n",
    "        lstm_out, _ = self.lstm(x)\n",
    "        # Use last time step from both directions\n",
    "        last_hidden = lstm_out[:, -1, :]  # shape: [batch_size, hidden_size * 2]\n",
    "        out = self.fc(last_hidden)\n",
    "        return out\n",
    "\n",
    "\n",
    "input_dim = X_train.shape[2]\n",
    "model = DiabetesBiLSTM(input_size=input_dim)"
   ]
  },
